Node Insert(Node head,int data) {
// This is a "method-only" submission. 
// You only need to complete this method. 
    Node newNode=new Node();
    newNode.data=data;
    
    if(head==null){
        head=newNode;
        return head;
    }
    
    else{
        Node current=head;
        while(current.next!=null){
            current=current.next;
        }
        current.next=newNode;
        return head;
    }
  
}
